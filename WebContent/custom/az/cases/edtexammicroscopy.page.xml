<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.0.xsd">
 <restrict>#{userSession.canEditCases}</restrict>
 <begin-conversation join="true"/>
 <param converterId="javax.faces.Integer" name="id" value="#{caseHome.id}"/>
 <param converterId="javax.faces.Integer" name="examid" value="#{examMicroscopyAZHome.idWithLog}"/>
 <navigation>
  <rule if="#{not caseHome.managed}">
   <end-conversation/>
   <redirect view-id="/cases/index.xhtml"/>
  </rule>
  <rule if-outcome="canceled">
   <end-conversation before-redirect="true"/>
   <redirect view-id="/cases/casedata.xhtml">
    <param converterId="javax.faces.Integer" name="id" value="#{caseHome.id}"/>
   </redirect>
  </rule>
  <rule if-outcome="persisted">
   <end-conversation/>
   <redirect view-id="/cases/casedata.xhtml">
    <param name="id" value="#{caseHome.id}"/>
   </redirect>
  </rule>
 </navigation>
</page>
