<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.0.xsd">

		<restrict>#{userSession.canEditCases}</restrict>
		<begin-conversation join="true"/>
		<param name="id" value="#{caseHome.id}" converterId="javax.faces.Integer"/>
		<param name="month" value="#{caseDispensingHomeNA.month}" converterId="javax.faces.Integer"/>
		<param name="year" value="#{caseDispensingHomeNA.year}" converterId="javax.faces.Integer"/>
		<navigation>
			<rule if-outcome="dispensing-saved">
				<end-conversation/>
				<redirect view-id="/custom/na/cases/casedata.xhtml">
					<param name="id" value="#{caseHome.id}" converterId="javax.faces.Integer"/>
				</redirect>
			</rule>
		</navigation>
</page>