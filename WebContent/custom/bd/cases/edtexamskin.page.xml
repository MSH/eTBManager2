<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.0.xsd">

	<restrict>#{caseHome.canEditExams}</restrict>
	<begin-conversation join="true" />
	<param name="id" value="#{caseHome.id}" converterId="javax.faces.Integer" />
	<param name="examid" value="#{examSkinHome.idWithLog}"
		converterId="javax.faces.Integer" />
	<navigation>
		<rule if="#{not caseHome.managed}">
			<end-conversation />
			<redirect view-id="/cases/index.xhtml" />
		</rule>

		<rule if-outcome="canceled">
			<end-conversation before-redirect="true" />
			<redirect view-id="/custom/bd/cases/casedata.xhtml">
				<param name="id" value="#{caseHome.id}" converterId="javax.faces.Integer" />
			</redirect>
		</rule>

		<rule if-outcome="persisted">
			<end-conversation />
			<redirect view-id="/custom/bd/cases/casedata.xhtml">
				<param name="id" value="#{caseHome.id}" converterId="javax.faces.Integer" />
			</redirect>
		</rule>
	</navigation>
</page>