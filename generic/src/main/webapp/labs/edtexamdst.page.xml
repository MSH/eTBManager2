<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.0.xsd">

	<restrict>#{userSession.canEditCases}</restrict>
	<begin-conversation join="true"/>
	<action execute="#{examDSTHome.initialize}" />
	<param name="id" value="#{sampleHome.id}" converterId="javax.faces.Integer"/>
	<param name="examid" value="#{examDSTHome.idWithLog}" converterId="javax.faces.Integer"/>
	<navigation>
		<rule if="#{not sampleHome.managed}">
			<end-conversation/>
			<redirect view-id="/labs/index.xhtml"/>
		</rule>
		
		<rule if-outcome="persisted">
			<redirect view-id="/labs/sampledata.xhtml">
				<param name="id" value="#{sampleHome.id}"/>
			</redirect>
		</rule>
	
		<rule if-outcome="canceled">
			<end-conversation before-redirect="true"/>
			<redirect view-id="/labs/sampledata.xhtml">
				<param name="id" value="#{sampleHome.id}" converterId="javax.faces.Integer"/>
			</redirect>
		</rule>
	</navigation>
</page>