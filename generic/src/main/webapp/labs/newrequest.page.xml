<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.0.xsd">

		<action execute="#{examRequestHome.init}"/>
		<param name="pacid" value="#{patientHome.id}" converterId="javax.faces.Integer"/>
		<begin-conversation join="true"/>

		<navigation>
			<rule if-outcome="patient-searching">
				<end-conversation before-redirect="true"/>
				<redirect view-id="/labs/newpac.xhtml">
				</redirect>
			</rule>

			<rule if-outcome="persisted">
				<end-conversation/>
				<redirect view-id="/labs/casesamples.xhtml">
					<param name="id" value="#{caseHome.id}"/>
				</redirect>
			</rule>
		</navigation>
	
</page>