<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.0.xsd">

    <action execute="#{examCultureHome.initializeForm}"/>

	<param name="id" value="#{caseHome.id}" converterId="javax.faces.Integer"/>
	<param name="examid" value="#{examCultureHome.idWithLog}" converterId="javax.faces.Integer"/>

		<navigation>
			<rule if-outcome="canceled">
				<redirect view-id="/cases/casedata.xhtml">
					<param name="id" value="#{caseHome.id}" converterId="javax.faces.Integer"/>
				</redirect>
			</rule>
			<rule if-outcome="persisted">
				<redirect view-id="/cases/casedata.xhtml">
					<param name="id" value="#{caseHome.id}" converterId="javax.faces.Integer"/>
				</redirect>
			</rule>
		</navigation>
</page>