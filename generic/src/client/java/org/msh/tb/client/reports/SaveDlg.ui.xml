<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui='urn:ui:com.google.gwt.uibinder'
             xmlns:g='urn:import:com.google.gwt.user.client.ui'
        >
	
	<ui:with field='msg' type='org.msh.tb.client.resources.ReportConstants'/>
	
	<g:DialogBox modal="true" autoHide="true" glassEnabled="true">
	    <g:caption><b>Save report</b></g:caption>
	    <g:VerticalPanel>
	        <g:HorizontalPanel styleName="modal-content">
	            <g:Label>Report title:</g:Label>
	            <g:TextBox ui:field="edtTitle" width="300px"/>
	        </g:HorizontalPanel>
	        <g:HTMLPanel styleName="modal-footer">
	            <g:Button ui:field="btnSave">Save</g:Button>
	            <g:Button ui:field="btnCancel">Cancel</g:Button>
	        </g:HTMLPanel>
	    </g:VerticalPanel>
	</g:DialogBox>
		
</ui:UiBinder>
