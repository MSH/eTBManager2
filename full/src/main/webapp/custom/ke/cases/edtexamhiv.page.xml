<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages"
      xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
      xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.0.xsd">

	<param name="id" value="#{caseHome.id}" converterId="javax.faces.Integer"/>
	<param name="examid" value="#{examHIVHome.idWithLog}" converterId="javax.faces.Integer"/>
	<navigation>
		<rule if="#{not caseHome.canEditExams}">
			<redirect view-id="/cases/casedata.xhtml">
			    <message>#{messages['exceptions.auth']}</message>
				<param name="id" value="#{caseHome.id}" />
			</redirect>
		</rule>
		
	    <rule if-outcome="persisted">
			<redirect view-id="/cases/casedata.xhtml">
				<param name="id" value="#{caseHome.id}"/>
			</redirect>
		</rule>
	</navigation>
</page>