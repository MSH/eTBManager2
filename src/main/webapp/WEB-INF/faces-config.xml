            <supported-locale>en_NG</supported-locale>
            <supported-locale>en_KH</supported-locale>
            <supported-locale>km</supported-locale>
            <supported-locale>pt_BR</supported-locale>
            <supported-locale>hy_AM</supported-locale>
            <supported-locale>es</supported-locale>
            <supported-locale>fr</supported-locale>
            <supported-locale>ro</supported-locale>
            <supported-locale>ru</supported-locale>
            <supported-locale>zh</supported-locale>
            <supported-locale>vi</supported-locale>
            <supported-locale>in</supported-locale>
            <supported-locale>uk_UA</supported-locale>
            <supported-locale>az_AZ</supported-locale>
        </locale-config>
    </application>
</faces-config>
