<?xml version="1.0" encoding="UTF-8"?>
<page xmlns="http://jboss.com/products/seam/pages"
 xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
 	   xsi:schemaLocation="http://jboss.com/products/seam/pages http://jboss.com/products/seam/pages-2.0.xsd">
 <action execute="#{quarterStockPositionHome.initializeEditing()}"/>
 <param name="selMed" value="#{quarterStockPositionHome.medicineId}" />
 <navigation>
 	<rule if-outcome="success">
		<end-conversation before-redirect="true"/>
		<redirect view-id="/custom/bd/medicines/quartelystqpos.xhtml">
			<param name="selQuarter" value="#{quarterStockPositionHome.selectedQuarter.getQuarter()}" />
			<param name="selYear" value="#{quarterStockPositionHome.selectedQuarter.getYear()}" />
		</redirect>
	</rule>
	<rule if-outcome="error">
		<redirect view-id="/custom/bd/medicines/quartelystqposedt.xhtml">
		</redirect>
	</rule>
</navigation>
</page>
